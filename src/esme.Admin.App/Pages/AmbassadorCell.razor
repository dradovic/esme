@inherits AmbassadorCellBase

@if (IsAmbassador.HasValue)
{
    @if (IsAmbassador.Value)
    {
        <button class='btn btn-sm btn-danger' @onclick="@DenyAmbassador">Deny</button>
    }
    else
    {
        <button class='btn btn-sm btn-success' @onclick="@GrantAmbassador">Grant</button>
    }
}
