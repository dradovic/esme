{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "vmsg@0.3.6",
      "files": [ "vmsg.js", "vmsg.wasm" ],
      "destination": "wwwroot/lib/vmsg"
    },
    {
      "library": "jquery@3.4.1",
      "files": [ "jquery.slim.min.js" ],
      "destination": "wwwroot/lib/jquery"
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "files": [ "css/bootstrap.min.css", "js/bootstrap.min.js" ],
      "destination": "wwwroot/lib/bootstrap"
    }
  ]
}